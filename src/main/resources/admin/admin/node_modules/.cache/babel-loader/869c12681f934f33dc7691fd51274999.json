      </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          :resizable=\"true\"\r\n          :sortable=\"false\"\r\n          prop=\"quxiaoshijian\"\r\n          label=\"取消时间\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.quxiaoshijian }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          :resizable=\"true\"\r\n          :sortable=\"false\"\r\n          prop=\"xuehao\"\r\n          label=\"学号\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.xuehao }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          :resizable=\"true\"\r\n          :sortable=\"false\"\r\n          prop=\"xingming\"\r\n          label=\"姓名\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.xingming }}\r\n          </template>\r\n        </el-table-column>\r\n        <!-- <el-table-column\r\n          :resizable=\"true\"\r\n          :sortable=\"false\"\r\n          prop=\"banji\"\r\n          label=\"班级\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.banji }}\r\n          </template>\r\n        </el-table-column> -->\r\n        <el-table-column\r\n          :resizable=\"true\"\r\n          :sortable=\"false\"\r\n          prop=\"shouji\"\r\n          label=\"手机\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.shouji }}\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          width=\"260\"\r\n          label=\"操作\"\r\n          fixed=\"right\"\r\n          class-name=\"action-column\"\r\n        >\r\n          <template #default=\"{ row }\">\r\n            <div class=\"action-btns\">\r\n              <el-button\r\n                v-if=\"isAuth('qiantuixinxi', '查看')\"\r\n                size=\"mini\"\r\n                icon=\"el-icon-view\"\r\n      