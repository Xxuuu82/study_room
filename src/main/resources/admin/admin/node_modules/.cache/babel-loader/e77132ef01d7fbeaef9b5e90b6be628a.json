"login\"\r\n            >\r\n            <!-- 验证码展示区域，点击可刷新 -->\r\n            <div\r\n              :style='{\"width\":\"35%\",\"marginLeft\":\"5%\",\"cursor\":\"pointer\",\"display\":\"flex\",\"justifyContent\":\"space-between\"}'\r\n              @click=\"getRandCode()\"\r\n            >\r\n              <span\r\n                v-for=\"(item, index) in codes\"\r\n                :key=\"index\"\r\n                :style='{\r\n                  \"color\": item.color,\r\n                  \"transform\": item.rotate,\r\n                  \"fontSize\": item.size,\r\n                  \"fontWeight\": \"bold\",\r\n                  \"display\": \"inline-block\"\r\n                }'\r\n              >\r\n                {{ item.num }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport menu from \"@/utils/menu\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      baseUrl: this.$base.url,\r\n      loginType: 1,\r\n      rulesForm: {\r\n        correctCode: \"\",\r\n        username: \"\",\r\n        password: \"\",\r\n        role: \"\",\r\n        code: \"\",\r\n      },\r\n      menus: [],\r\n      roles: [],\r\n      tableName: \"\",\r\n      codes: [\r\n        {\r\n          num: 1,\r\n          color: \"#000\",\r\n          rotate: \"10deg\",\r\n          size: \"16px\",\r\n        },\r\n        {\r\n          num: 2,\r\n          color: \"#000\",\r\n          rotate: \"10deg\",\r\n          size: \"16px\",\r\n        },\r\n        {\r\n          num: 3,\r\n          color: \"#000\",\r\n          rotate: \"10deg\",\r\n          size: \"16px\",\r\n        },\r\n        {\r\n          num: 4,\r\n          color: \"#000\",\r\n          rotate: \"10deg\",\r\n          size: \"16px\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    let menus = menu.list();\r\n    this.menus = menus;\r\n    this.getRandCode();\r\n    for (let i 