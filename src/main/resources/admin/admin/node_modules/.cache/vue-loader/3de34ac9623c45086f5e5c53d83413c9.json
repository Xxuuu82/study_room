/>\r\n        <el-option label=\"大声喧哗\" value=\"xuanhua\" />\r\n        <el-option label=\"随地大小便\" value=\"suibiaobian\" />\r\n        <el-option label=\"其他\" value=\"other\" />\r\n      </el-select>\r\n      <el-button type=\"primary\" @click=\"fetchWeiguiList\" style=\"margin-left: 10px;\">查询</el-button>\r\n      <el-button type=\"success\" @click=\"openAddDialog\" style=\"margin-left: 10px;\">新增违规记录</el-button>\r\n    </div>\r\n\r\n    <!-- 违规记录列表 -->\r\n    <el-table :data=\"weiguiList\" border style=\"width: 100%; margin-top: 20px;\">\r\n      <el-table-column prop=\"xuehao\" label=\"学号\" />\r\n      <el-table-column prop=\"xingming\" label=\"姓名\" />\r\n      <el-table-column prop=\"weiguiLeixingName\" label=\"违规类型\" />\r\n      <el-table-column prop=\"weiguiShijian\" label=\"违规时间\" />\r\n      <el-table-column prop=\"zixishimingcheng\" label=\"违规自习室\" />\r\n      <el-table-column prop=\"weiguiBeizhu\" label=\"违规详情\" />\r\n      <el-table-column label=\"操作\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"danger\" @click=\"deleteWeigui(scope.row.id)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 新增违规记录弹窗 -->\r\n    <el-dialog title=\"新增违规记录\" v-model=\"addDialogVisible\" width=\"50%\">\r\n      <el-form :model=\"addForm\" label-width=\"120px\">\r\n        <el-form-item label=\"学号\">\r\n          <el-input v-model=\"addForm.xuehao\" placeholder=\"请输入学生学号\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"addForm.xingming\" placeholder=\"请输入学生姓名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"违规类型\">\r\n          <el-select v-model=\"addForm.weiguiLeixing\" placeholder=\"选择违规类型\" @change=\"handleLeixingChange\">\r\n            <el-option label=\"教室乱吃乱喝\" value=\"chifan\" />\r\n            <el-option label=\"未签到\" value=\"weiqiandao\" />\r\n            <el-option label=\"大声喧哗\" value=\"xuanhua\" />\r\n            <el-option label=\"随地大小便\" value=\"suibiaobian\" />\r\n            <el-option label=\"其他\" value=\"other\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"违规类型名称\" v-if=\"addForm.weiguiLeixing === 'other'\">\r\n          <el-input v-model=\"addForm.weiguiLeixingName\" placeholder=\"输入其他违规类型名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"违规时间\">\r\n          <el-date-picker v-model=\"addForm.weiguiShijian\" type=\"datetime\" placeholder=\"选择违规时间\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"违规自习室\">\r\n          <el-input v-model=\"addForm.zixishimingcheng\" placeholder=\"输入违规自习室名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"违规详情\">\r\n          <el-input v-model=\"addForm.weiguiBeizhu\" type=\"textarea\" placeholder=\"输入违规详情\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <el-button @click=\"addDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitAddForm\">确定</el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'WeiguiRecord',\r\n  data() {\r\n    return {\r\n      searchXuehao: '',\r\n      searchLeixing: '',\r\n      weiguiList: [],\r\n      addDialogVisible: false,\r\n      addForm: {\r\n        xuehao: '',\r\n        xingming: '',\r\n        weiguiLeixing: '',\r\n        weiguiLeixingName: '',\r\n        weiguiShijian: '',\r\n        zixishimingcheng: '',\r\n        weiguiBeizhu: ''\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchWeiguiList()\r\n  },\r\n  methods: {\r\n    // 获取违规记录列表\r\n    fetchWeiguiList() {\r\n      this.$axios.get('/weigui/list', {\r\n        params: {\r\n          xuehao: this.searchXuehao,\r\n          weiguiLeixing: this.searchLeixing\r\n        }\r\n      }).then(res => {\r\n        this.weiguiList = res.data.data\r\n      })\r\n    },\r\n    // 打开新增弹窗\r\n    openAddDialog() {\r\n      this.addForm = {\r\n        xuehao: '',\r\n        xingming: '',\r\n        weiguiLeixing: '',\r\n        weiguiLeixingName: '',\r\n        weiguiShijian: '',\r\n        zixishimingcheng: '',\r\n        weiguiBeizhu: ''\r\n      }\r\n      this.addDialogVisible = true\r\n    },\r\n    // 违规类型切换（其他类型需手动输入名称）\r\n    handleLeixingChange(val) {\r\n      if (val !== 'other') {\r\n        this.addForm.weiguiLeixingName = this.$refs.leixingSelect.currentLabel\r\n      } else {\r\n        this.addForm.weiguiLeixingName = ''\r\n      }\r\n    },\r\n    // 提交新增表单\r\n    submitAddForm() {\r\n      this.$axios.post('/weigui/add', this.addForm).then(res => {\r\n        if (res.data.code === 200) {\r\n          this.$message.success('新增违规记录成功')\r\n          this.addDialogVisible = false\r\n          this.fetchWeiguiList()\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n      })\r\n    },\r\n    // 删除违规记录\r\n    deleteWeigui(id) {\r\n      this.$confirm('此操作将删除该违规记录, 是否继续?', '提示', {\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$axios.post('/weigui/delete', { id }).then(res => {\r\n          if (res.data.code === 200) {\r\n            this.$message.success('删除成功!')\r\n            this.fetchWeiguiList()\r\n          } else {\r\n            this.$message.error(res.data.msg)\r\n          }\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.search-bar {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"]}]}