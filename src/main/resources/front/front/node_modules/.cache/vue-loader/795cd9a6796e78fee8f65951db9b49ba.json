;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAinBA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"add.vue","sourceRoot":"src/pages/zixishi","sourcesContent":["<template>\r\n  <div style=\"width: 100%; min-height: 100vh; padding: 0 10px; box-sizing: border-box; background: #bfd2fc;\">\r\n    <!-- 面包屑导航 -->\r\n    <div :style='{\"width\":\"100%\",\"padding\":\"20px\",\"margin\":\"10px auto\",\"borderRadius\":\"27px\",\"background\":\"#bfd2fc\"}' class=\"breadcrumb-preview\">\r\n      <el-breadcrumb :separator=\"'Ξ'\" :style='{\"fontSize\":\"14px\",\"lineHeight\":\"1\"}'>\r\n        <el-breadcrumb-item to=\"/\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item to=\"/index/zixishi\">自习室</el-breadcrumb-item>\r\n        <el-breadcrumb-item>预约信息</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n\r\n    <!-- 预约表单容器 -->\r\n    <div class=\"detail-preview\" :style='{\r\n      width: \"100%\",\r\n      maxWidth: \"1000px\",\r\n      margin: \"20px auto\",\r\n      position: \"relative\",\r\n      background: \"#fff\",\r\n      boxShadow: \"0px 4px 10px 0px rgba(0,0,0,0.302)\",\r\n      borderRadius: \"8px\",\r\n      boxSizing: \"border-box\",\r\n      padding: \"30px\"\r\n    }'>\r\n\r\n      <!-- 页面标题 -->\r\n      <div class=\"page-title\" :style=\"{\r\n        marginBottom: '30px',\r\n        paddingBottom: '15px',\r\n        borderBottom: '1px solid #eee'\r\n      }\">\r\n        <div :style=\"{\r\n          fontSize: '20px',\r\n          fontWeight: '600',\r\n          color: '#333',\r\n          display: 'flex',\r\n          alignItems: 'center',\r\n          gap: '10px'\r\n        }\">\r\n          <i class=\"el-icon-document\" :style=\"{color: '#2E61E1'}\"></i>\r\n          预约信息填写\r\n        </div>\r\n        <div :style=\"{\r\n          fontSize: '14px',\r\n          color: '#666',\r\n          marginTop: '8px'\r\n        }\">\r\n          请填写自习室预约相关信息\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 预约表单 -->\r\n      <el-form\r\n          class=\"add-update-preview\"\r\n          ref=\"ruleForm\"\r\n          :model=\"ruleForm\"\r\n          :rules=\"rules\"\r\n          label-width=\"120px\"\r\n          :style='{\"padding\":\"10px\"}'\r\n      >\r\n        <!-- 响应式表单布局 -->\r\n        <div :style=\"{\r\n          display: 'grid',\r\n          gridTemplateColumns: 'repeat(auto-fit, minmax(350px, 1fr))',\r\n          gap: '20px',\r\n          marginBottom: '20px'\r\n        }\">\r\n          <!-- 名称 -->\r\n          <el-form-item label=\"名称\" prop=\"mingcheng\" :style='{\"margin\":\"0\"}'>\r\n            <el-input\r\n                v-model=\"ruleForm.mingcheng\"\r\n                placeholder=\"请输入自习室名称\"\r\n                clearable\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f8f9fa',\r\n                border: '2px solid #2e61e1',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px'\r\n              }\"\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 规模 -->\r\n          <el-form-item label=\"规模\" prop=\"guimo\" :style='{\"margin\":\"0\"}'>\r\n            <el-select\r\n                v-model=\"ruleForm.guimo\"\r\n                placeholder=\"请选择规模\"\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f8f9fa',\r\n                border: '2px solid #2e61e1',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px'\r\n              }\"\r\n            >\r\n              <el-option\r\n                  v-for=\"(item,index) in guimoOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\"\r\n                  :style=\"{padding: '10px'}\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <!-- 座位 -->\r\n          <el-form-item label=\"座位\" prop=\"zuowei\" :style='{\"margin\":\"0\"}'>\r\n            <el-input\r\n                v-model=\"ruleForm.zuowei\"\r\n                placeholder=\"座位信息\"\r\n                clearable\r\n                readonly\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f0f2f5',\r\n                border: '2px solid #dcdfe6',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px',\r\n                color: '#666'\r\n              }\"\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 位置 -->\r\n          <el-form-item label=\"位置\" prop=\"weizhi\" :style='{\"margin\":\"0\"}'>\r\n            <el-input\r\n                v-model=\"ruleForm.weizhi\"\r\n                placeholder=\"请输入自习室位置\"\r\n                clearable\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f8f9fa',\r\n                border: '2px solid #2e61e1',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px'\r\n              }\"\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 设施 -->\r\n          <el-form-item label=\"设施\" prop=\"sheshi\" :style='{\"margin\":\"0\"}'>\r\n            <el-input\r\n                v-model=\"ruleForm.sheshi\"\r\n                placeholder=\"请输入自习室设施\"\r\n                clearable\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f8f9fa',\r\n                border: '2px solid #2e61e1',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px'\r\n              }\"\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 是否有课 -->\r\n          <el-form-item label=\"是否有课\" prop=\"shifouyouke\" :style='{\"margin\":\"0\"}'>\r\n            <el-select\r\n                v-model=\"ruleForm.shifouyouke\"\r\n                placeholder=\"请选择是否有课\"\r\n                :style=\"{\r\n                width: '100%',\r\n                background: '#f8f9fa',\r\n                border: '2px solid #2e61e1',\r\n                borderRadius: '6px',\r\n                fontSize: '14px',\r\n                height: '40px'\r\n              }\"\r\n            >\r\n              <el-option\r\n                  v-for=\"(item,index) in shifouyoukeOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\"\r\n                  :style=\"{padding: '10px'}\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </div>\r\n\r\n        <!-- 时间选择区域 -->\r\n        <div :style=\"{\r\n          background: '#f8f9fa',\r\n          padding: '20px',\r\n          borderRadius: '8px',\r\n          marginBottom: '20px',\r\n          border: '1px solid #e9ecef'\r\n        }\">\r\n          <div :style=\"{\r\n            fontSize: '16px',\r\n            fontWeight: '500',\r\n            color: '#333',\r\n            marginBottom: '15px',\r\n            display: 'flex',\r\n            alignItems: 'center',\r\n            gap: '8px'\r\n          }\">\r\n            <i class=\"el-icon-time\" :style=\"{color: '#2E61E1'}\"></i>\r\n            预约时间选择\r\n          </div>\r\n\r\n          <div :style=\"{\r\n            display: 'grid',\r\n            gridTemplateColumns: 'repeat(auto-fit, minmax(300px, 1fr))',\r\n            gap: '20px'\r\n          }\">\r\n            <!-- 开始时间 -->\r\n            <el-form-item label=\"预约开始时间\" prop=\"startTime\" :style='{\"margin\":\"0\"}'>\r\n              <el-date-picker\r\n                  v-model=\"ruleForm.startTime\"\r\n                  type=\"datetime\"\r\n                  placeholder=\"选择开始时间\"\r\n                  clearable\r\n                  :style=\"{\r\n                    width: '100%',\r\n                    background: '#fff',\r\n                    border: '2px solid #2e61e1',\r\n                    borderRadius: '6px',\r\n                    fontSize: '14px',\r\n                    height: '40px'\r\n                  }\"\r\n              ></el-date-picker>\r\n            </el-form-item>\r\n\r\n            <!-- 结束时间 -->\r\n            <el-form-item label=\"预约结束时间\" prop=\"endTime\" :style='{\"margin\":\"0\"}'>\r\n              <el-date-picker\r\n                  v-model=\"ruleForm.endTime\"\r\n                  type=\"datetime\"\r\n                  placeholder=\"选择结束时间\"\r\n                  clearable\r\n                  :style=\"{\r\n                    width: '100%',\r\n                    background: '#fff',\r\n                    border: '2px solid #2e61e1',\r\n                    borderRadius: '6px',\r\n                    fontSize: '14px',\r\n                    height: '40px'\r\n                  }\"\r\n              ></el-date-picker>\r\n            </el-form-item>\r\n          </div>\r\n\r\n          <!-- 时间提示 -->\r\n          <div :style=\"{\r\n            marginTop: '15px',\r\n            padding: '10px',\r\n            background: '#fff',\r\n            borderRadius: '6px',\r\n            fontSize: '13px',\r\n            color: '#666',\r\n            borderLeft: '3px solid #2E61E1'\r\n          }\">\r\n            <i class=\"el-icon-info\" :style=\"{marginRight: '6px', color: '#2E61E1'}\"></i>\r\n            请合理安排预约时间，确保开始时间早于结束时间。\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 详情编辑区域 -->\r\n        <div :style=\"{\r\n          marginBottom: '20px'\r\n        }\">\r\n          <div :style=\"{\r\n            fontSize: '16px',\r\n            fontWeight: '500',\r\n            color: '#333',\r\n            marginBottom: '15px',\r\n            display: 'flex',\r\n            alignItems: 'center',\r\n            gap: '8px'\r\n          }\">\r\n            <i class=\"el-icon-edit\" :style=\"{color: '#2E61E1'}\"></i>\r\n            预约详情\r\n          </div>\r\n\r\n          <el-form-item label=\"详情\" prop=\"xiangqing\" :style='{\"margin\":\"0\"}'>\r\n            <editor\r\n                :style='{\r\n                  minHeight: \"300px\",\r\n                  padding: \"15px\",\r\n                  boxShadow: \"0 0 6px rgba(46, 97, 225, 0.1)\",\r\n                  margin: \"0\",\r\n                  border: \"2px solid #2e61e1\",\r\n                  backgroundColor: \"#fff\",\r\n                  borderRadius: \"6px\",\r\n                  color: \"#000\",\r\n                  width: \"100%\",\r\n                  height: \"auto\"\r\n                }'\r\n                v-model=\"ruleForm.xiangqing\"\r\n                class=\"editor\"\r\n                action=\"file/upload\">\r\n            </editor>\r\n          </el-form-item>\r\n        </div>\r\n\r\n        <!-- 操作按钮 -->\r\n        <el-form-item :style='{\"padding\":\"0\",\"margin\":\"30px 0 0\",\"textAlign\":\"center\"}'>\r\n          <div :style=\"{\r\n            display: 'flex',\r\n            justifyContent: 'center',\r\n            gap: '20px',\r\n            flexWrap: 'wrap'\r\n          }\">\r\n            <el-button\r\n                type=\"primary\"\r\n                @click=\"onSubmit\"\r\n                :style=\"{\r\n                border: 'none',\r\n                cursor: 'pointer',\r\n                padding: '0 30px',\r\n                boxShadow: '0px 2px 4px 0px rgba(46, 97, 225, 0.3)',\r\n                outline: 'none',\r\n                color: '#fff',\r\n                borderRadius: '30px',\r\n                background: '#2E61E1',\r\n                fontSize: '14px',\r\n                height: '44px',\r\n                transition: 'all 0.2s',\r\n                minWidth: '140px'\r\n              }\"\r\n                @mouseenter=\"e => e.target.style.transform='translateY(-2px)'\"\r\n                @mouseleave=\"e => e.target.style.transform='translateY(0)'\"\r\n            >\r\n              <i class=\"el-icon-check\" :style=\"{marginRight: '8px'}\"></i>\r\n              提交预约\r\n            </el-button>\r\n\r\n            <el-button\r\n                @click=\"back()\"\r\n                :style=\"{\r\n                border: '2px solid #2E61E1',\r\n                cursor: 'pointer',\r\n                padding: '0 30px',\r\n                outline: 'none',\r\n                color: '#2E61E1',\r\n                borderRadius: '30px',\r\n                background: '#fff',\r\n                fontSize: '14px',\r\n                height: '44px',\r\n                transition: 'all 0.2s',\r\n                minWidth: '140px'\r\n              }\"\r\n                @mouseenter=\"e => e.target.style.background='#f8f9fa'\"\r\n                @mouseleave=\"e => e.target.style.background='#fff'\"\r\n            >\r\n              <i class=\"el-icon-back\" :style=\"{marginRight: '8px'}\"></i>\r\n              返回上一页\r\n            </el-button>\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: '',\r\n      baseUrl: '',\r\n      ro:{\r\n        mingcheng : false,\r\n        guimo : false,\r\n        zuowei : false,\r\n        weizhi : false,\r\n        sheshi : false,\r\n        startTime : false,\r\n        endTime : false,\r\n        shifouyouke : false,\r\n        xiangqing : false,\r\n      },\r\n      type: '',\r\n      userTableName: localStorage.getItem('UserTableName'),\r\n      ruleForm: {\r\n        mingcheng: '',\r\n        guimo: '',\r\n        zuowei: '20',\r\n        weizhi: '',\r\n        sheshi: '',\r\n        startTime: '',\r\n        endTime: '',\r\n        shifouyouke: '是',\r\n        xiangqing: '',\r\n      },\r\n      guimoOptions: [],\r\n      shifouyoukeOptions: [],\r\n      rules: {\r\n        mingcheng: [\r\n          { required: true, message: '名称不能为空', trigger: 'blur' },\r\n        ],\r\n        guimo: [\r\n        ],\r\n        zuowei: [\r\n          { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n        ],\r\n        weizhi: [\r\n          { required: true, message: '位置不能为空', trigger: 'blur' },\r\n        ],\r\n        sheshi: [\r\n        ],\r\n        startTime: [\r\n          { required: true, message: '预约开始时间不能为空', trigger: 'change' },\r\n        ],\r\n        endTime: [\r\n          { required: true, message: '预约结束时间不能为空', trigger: 'change' },\r\n          { validator: this.checkEndTime, trigger: 'change' }\r\n        ],\r\n        shifouyouke: [\r\n        ],\r\n        xiangqing: [\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  computed: {},\r\n  created() {\r\n    let type = this.$route.query.type ? this.$route.query.type : '';\r\n    this.init(type);\r\n    this.baseUrl = this.$config.baseUrl;\r\n  },\r\n  methods: {\r\n    // 校验结束时间大于开始时间\r\n    checkEndTime(rule, value, callback) {\r\n      if (!value) {\r\n        callback(new Error('预约结束时间不能为空'));\r\n      } else if (this.ruleForm.startTime && new Date(value) <= new Date(this.ruleForm.startTime)) {\r\n        callback(new Error('预约结束时间必须晚于开始时间'));\r\n      } else {\r\n        callback();\r\n      }\r\n    },\r\n    getMakeZero(s) {\r\n      return s < 10 ? '0' + s : s;\r\n    },\r\n    // 下载\r\n    download(file){\r\n      window.open(`${file}`)\r\n    },\r\n    // 初始化\r\n    init(type) {\r\n      this.type = type;\r\n      if(type=='cross'){\r\n        var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n        for (var o in obj){\r\n          if(o=='mingcheng'){\r\n            this.ruleForm.mingcheng = obj[o];\r\n            this.ro.mingcheng = true;\r\n            continue;\r\n          }\r\n          if(o=='guimo'){\r\n            this.ruleForm.guimo = obj[o];\r\n            this.ro.guimo = true;\r\n            continue;\r\n          }\r\n          if(o=='zuowei'){\r\n            this.ruleForm.zuowei = obj[o];\r\n            this.ro.zuowei = true;\r\n            continue;\r\n          }\r\n          if(o=='weizhi'){\r\n            this.ruleForm.weizhi = obj[o];\r\n            this.ro.weizhi = true;\r\n            continue;\r\n          }\r\n          if(o=='sheshi'){\r\n            this.ruleForm.sheshi = obj[o];\r\n            this.ro.sheshi = true;\r\n            continue;\r\n          }\r\n          // 兼容原有可约时间数据（如有）\r\n          if(o=='keyueshijian'){\r\n            if(obj[o] && obj[o].indexOf('-') > -1) {\r\n              const [start, end] = obj[o].split('-');\r\n              this.ruleForm.startTime = start;\r\n              this.ruleForm.endTime = end;\r\n            }\r\n            this.ro.startTime = true;\r\n            this.ro.endTime = true;\r\n            continue;\r\n          }\r\n          if(o=='shifouyouke'){\r\n            this.ruleForm.shifouyouke = obj[o];\r\n            this.ro.shifouyouke = true;\r\n            continue;\r\n          }\r\n          if(o=='xiangqing'){\r\n            this.ruleForm.xiangqing = obj[o];\r\n            this.ro.xiangqing = true;\r\n            continue;\r\n          }\r\n        }\r\n      }\r\n      // 获取用户信息\r\n      this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n        if (res.data.code == 0) {\r\n          var json = res.data.data;\r\n        }\r\n      });\r\n      this.guimoOptions = \"大型,中型,小型\".split(',')\r\n      this.shifouyoukeOptions = \"是,否\".split(',')\r\n    },\r\n\r\n    // 多级联动参数\r\n    info(id) {\r\n      this.$http.get(`zixishi/detail/${id}`, {emulateJSON: true}).then(res => {\r\n        if (res.data.code == 0) {\r\n          this.ruleForm = res.data.data;\r\n        }\r\n      });\r\n    },\r\n    // 提交\r\n    onSubmit() {\r\n      //更新跨表属性\r\n      var crossuserid;\r\n      var crossrefid;\r\n      var crossoptnum;\r\n      this.$refs[\"ruleForm\"].validate(valid => {\r\n        if(valid) {\r\n          if(this.type=='cross'){\r\n            var statusColumnName = localStorage.getItem('statusColumnName');\r\n            var statusColumnValue = localStorage.getItem('statusColumnValue');\r\n            if(statusColumnName && statusColumnName!='') {\r\n              var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n              if(!statusColumnName.startsWith(\"[\")) {\r\n                for (var o in obj){\r\n                  if(o==statusColumnName){\r\n                    obj[o] = statusColumnValue;\r\n                  }\r\n                }\r\n                var table = localStorage.getItem('crossTable');\r\n                console.log('s');\r\n                this.$http.post(table+'/update', obj).then(res => {});\r\n              } else {\r\n                crossuserid=Number(localStorage.getItem('userid'));\r\n                crossrefid=obj['id'];\r\n                crossoptnum=localStorage.getItem('statusColumnName');\r\n                crossoptnum=crossoptnum.replace(/\\[/,\"\").replace(/\\]/,\"\");\r\n              }\r\n            }\r\n          }\r\n          if(crossrefid && crossuserid) {\r\n            this.ruleForm.crossuserid=crossuserid;\r\n            this.ruleForm.crossrefid=crossrefid;\r\n            var params = {\r\n              page: 1,\r\n              limit: 10,\r\n              crossuserid:crossuserid,\r\n              crossrefid:crossrefid,\r\n            }\r\n            this.$http.get('zixishi/list', {\r\n              params: params\r\n            }).then(res => {\r\n              if(res.data.data.total>=crossoptnum) {\r\n                this.$message({\r\n                  message: localStorage.getItem('tips'),\r\n                  type: 'success',\r\n                  duration: 1500,\r\n                });\r\n                return false;\r\n              } else {\r\n                this.$http.post('zixishi/add', this.ruleForm).then(res => {\r\n                  if (res.data.code == 0) {\r\n                    this.$message({\r\n                      message: '预约成功',\r\n                      type: 'success',\r\n                      duration: 1500,\r\n                      onClose: () => {\r\n                        this.$router.go(-1);\r\n                      }\r\n                    });\r\n                  } else {\r\n                    this.$message({\r\n                      message: res.data.msg,\r\n                      type: 'error',\r\n                      duration: 1500\r\n                    });\r\n                  }\r\n                });\r\n              }\r\n            });\r\n          } else {\r\n            this.$http.post('zixishi/add', this.ruleForm).then(res => {\r\n              if (res.data.code == 0) {\r\n                this.$message({\r\n                  message: '预约成功',\r\n                  type: 'success',\r\n                  duration: 1500,\r\n                  onClose: () => {\r\n                    this.$router.go(-1);\r\n                  }\r\n                });\r\n              } else {\r\n                this.$message({\r\n                  message: res.data.msg,\r\n                  type: 'error',\r\n                  duration: 1500\r\n                });\r\n              }\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    // 获取uuid\r\n    getUUID () {\r\n      return new Date().getTime();\r\n    },\r\n    // 返回\r\n    back() {\r\n      this.$router.go(-1);\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n// 全局盒模型\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n\r\n// 面包屑样式\r\