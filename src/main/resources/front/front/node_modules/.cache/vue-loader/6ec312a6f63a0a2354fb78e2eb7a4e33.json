  <!-- 预约单号 -->\r\n      <el-form-item label=\"预约单号\">\r\n        <el-input v-model=\"ruleForm.yuyuedanhao\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 预约名称 -->\r\n      <el-form-item label=\"预约名称\">\r\n        <el-input v-model=\"ruleForm.mingcheng\" readonly></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- 图片显示（新增） -->\r\n      <el-form-item label=\"图片\" v-if=\"ruleForm.tupian\">\r\n        <div style=\"margin-top: 10px;\">\r\n          <img\r\n              v-if=\"ruleForm.tupian.substring(0, 4) == 'http'\"\r\n              :src=\"ruleForm.tupian.split(',')[0]\"\r\n              style=\"width: 100px; height: 100px; object-fit: cover; border-radius: 4px;\"\r\n          />\r\n          <img\r\n              v-else\r\n              :src=\"$base.url + ruleForm.tupian.split(',')[0]\"\r\n              style=\"width: 100px; height: 100px; object-fit: cover; border-radius: 4px;\"\r\n          />\r\n        </div>\r\n      </el-form-item>\r\n\r\n      <!-- 预约时间 -->\r\n      <el-form-item label=\"预约时间\">\r\n        <el-input v-model=\"ruleForm.yuyueshijian\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 取消时间 -->\r\n      <el-form-item label=\"取消时间\">\r\n        <el-input v-model=\"ruleForm.quxiaoshijian\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 学号 -->\r\n      <el-form-item label=\"学号\">\r\n        <el-input v-model=\"ruleForm.xuehao\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 姓名 -->\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"ruleForm.xingming\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 手机 -->\r\n      <el-form-item label=\"手机\">\r\n        <el-input v-model=\"ruleForm.shouji\" readonly></el-input>\r\n      </el-form-item>\r\n      <!-- 取消原因（重点） -->\r\n      <el-form-item label=\"取消原因\">\r\n        <el-input\r\n            type=\"textarea\"\r\n            :rows=\"4\"\r\n            v-model=\"ruleForm.quxiaoyuanyin\"\r\n            readonly\r\n            style=\"white-space: pre-line;\"\r\n        ></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"QuxiaoyuyueDetail\",\r\n  props: {\r\n    id: {\r\n      type: [String, Number],\r\n      required: true,\r\n      validator: (val) => {\r\n        return val !== null && val !== '';\r\n      }\r\n    },\r\n    close: {\r\n      type: Function,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      ruleForm: {\r\n        yuyuedanhao: \"\",\r\n        mingcheng: \"\",\r\n        tupian: \"\",\r\n        yuyueshijian: \"\",\r\n        quxiaoshijian: \"\",\r\n        xuehao: \"\",\r\n        xingming: \"\",\r\n        shouji: \"\",\r\n        quxiaoyuanyin: \"\"\r\n      },\r\n      loading: false\r\n    };\r\n  },\r\n  watch: {\r\n    // 监听ID变化，重新加载数据\r\n    id(newVal) {\r\n      if (newVal) {\r\n        this.getDetailData();\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getDetailData();\r\n  },\r\n  methods: {\r\n    // 获取详情数据\r\n    getDetailData() {\r\n      if (!this.id) return;\r\n\r\n      this.loading = true;\r\n      this.$http({\r\n        url: `quxiaoyuyue/info/${this.id}`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        this.loading = false;\r\n        if (data && data.code === 0) {\r\n          this.ruleForm = { ...data.data };\r\n        } else {\r\n          this.$message.error(data.msg || \"获取详情失败\");\r\n          this.close();\r\n        }\r\n      }).catch(() => {\r\n        this.loading = false;\r\n        this.$message.error(\"网络异常，获取详情失败\");\r\n        this.close();\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.detail-container {\r\n  padding: 10px;\r\n  box-sizing: border-box;\r\n}\r\n.vertical-form {\r\n  width: 100%;\r\n}\r\n.vertical-form .el-form-item {\r\n  margin-bottom: 20px;\r\n  width: 100%;\r\n}\r\n.vertical-form .el-input {\r\n  width: 100%;\r\n}\r\n.vertical-form .el-textarea {\r\n  width: 100%;\r\n}\r\n/* 只读输入框样式优化 */\r\n:deep(.el-input__inner[readonly]) {\r\n  background-color: #f5f7fa;\r\n  cursor: not-allowed;\r\n}\r\n/* 文本区域样式 */\r\n:deep(.el-textarea__inner[readonly]) {\r\n  background-color: #f5f7fa;\r\n  cursor: not-allowed;\r\n  white-space: pre-line;\r\n  line-height: 1.5;\r\n}\r\n</style>"]}]}